{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended",  "airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": ["error", {
         "allow": ["_id"]
      }]
    }
}
